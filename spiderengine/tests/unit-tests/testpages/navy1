function checkform() {
	var varSendMail = document.cmtForm.txtSendEmail.value;
	var varComment = document.cmtForm.txtComment.value;
           
	if (varComment.length < 10) {
		alert("Your comment is too short.  Please elaborate...")
		return false;
	}
	
	if (echeck(varSendMail) == false) {
		alert("A valid email address is required to verify your comment.  Contributer email addresses are never published.");
		return false;
	}
	return true;
}      

function echeck(str) {
		var at="@"
		var dot="."
		var lat=str.indexOf(at)
		var lstr=str.length
		var ldot=str.indexOf(dot)
		if (str.indexOf(at)==-1){
		   return false;
		}

		if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
		   return false;
		}

		if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
		    return false;
		}

		 if (str.indexOf(at,(lat+1))!=-1){
		    return false;
		 }

		 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		    return false;
		 }

		 if (str.indexOf(dot,(lat+2))==-1){
		    return false;
		 }
		
		 if (str.indexOf(" ")!=-1){
		    return false;
		 }
 		 return true;				
}

function limitText(limitField, limitCount, limitNum) {
	if (limitField.value.length > limitNum) {
		limitField.value = limitField.value.substring(0, limitNum);
	} 
	else if (limitField.value.length > limitNum - 100) {
		limitCount.value = limitNum - limitField.value.length;
		document.getElementById("limitWarn").style.display = "block";
	}
	else {
		limitCount.value = limitNum - limitField.value.length;
		document.getElementById("limitWarn").style.display = "none";
	}
}

function navPhoto(target, imgTotal) {
	for (i=1; i<=imgTotal; i++) {
	  document.getElementById("storyImage" + i).style.display = "none";
	}
	document.getElementById("storyImage" + target).style.display = "block";
}
