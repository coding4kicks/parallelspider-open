var extImageCount = 0;
// The following function adds the exterior image to the exterior dimensions table after creation
function addExtImage(data){
var containerTarget = document.getElementById("exteriorTable");
var tableObj = containerTarget.firstChild;
var targetTable = containerTarget.getElementsByTagName("table");
var newRow = targetTable[0].insertRow(1);
// Insert a cell in the row at index 0
var newCell = newRow.insertCell(0);
// Append a text node to the cell
var newDiv = document.createElement('div');
newDiv.innerHTML = data;
newCell.appendChild(newDiv);
}
</script>
