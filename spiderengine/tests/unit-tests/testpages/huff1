<script type="text/javascript">

  (function() {

    var $ = jQuery;
    var $widget = $('#hpin-big-results-rr');
    var widget_hash = '11111111111111111111111111111111';
    var widget_timestamp = '1354135978';
    var $flash = $('#hpin-big-results-rr');
    var ajax_stop = '11111111111111111111111111111111';
    var interval_id = null;

    var flash = function(html) {
      $flash.fadeOut(150, function() {
        if (html)
          $widget.html(html);
        $(this).fadeIn(150);
      });
    };

    var pollWidget = function() {
      $.ajax({
        url: '/hpin/callback.php?cb=results_2012_right_rail_big&ct=30',
        success: function(data) {
          var new_timestamp = data.match(/<!--GENERATED_AT:[^>]*-->/g)[0].substr(17, 10);
          var new_hash = data.match(/<!--SUMMARY_HASH:[^>]*-->/g)[0].substr(17, 32);
          if (new_hash != widget_hash && new_hash != ajax_stop && new_timestamp > widget_timestamp) {
            flash(data);
            widget_timestamp = new_timestamp;
            widget_hash = new_hash;
          }
          if (new_hash == ajax_stop && interval_id) {
            clearInterval(interval_id);
            interval_id = null;
          }
        }
      });
    };

    if (widget_hash != ajax_stop) {
      interval_id = setInterval(pollWidget, 30000);
    }
    flash();
    $('a[href^="http://elections.huffingtonpost.com/2012/results"]').attr("target", "hp2012results");

  }).call(this);

</script>
<!-- End elections.huffingtonpost.com -->
