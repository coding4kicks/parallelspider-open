<script type="text/javascript">
     $("div.slideContainer").children("div.slide").eq(0).css("width", "830px").addClass("expanded");
     $("div.slideContainer").delegate("div.tabTitle", "click", function () {
         var clickedslide = $(this).closest("div.slide");
         if (clickedslide.hasClass("expanded")) { return false; }
         $("div.slideContainer").children("div.expanded").animate({ width: "30px" }, 400).removeClass("expanded");
         clickedslide.animate({ width: "830px" }, 400).addClass("expanded");
     });
</script>
