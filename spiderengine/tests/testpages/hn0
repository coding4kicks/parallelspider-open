<script type="text/javascript">
function byId(id) {
  return document.getElementById(id);
}

function vote(node) {
  var v = node.id.split(/_/);   // {'up', '123'}
  var item = v[1]; 

  // hide arrows
  byId('up_'   + item).style.visibility = 'hidden';
  byId('down_' + item).style.visibility = 'hidden';

  // ping server
  var ping = new Image();
  ping.src = node.href;

  return false; // cancel browser nav
} </script><title>Hacker News</title></head><body><center><table border=0 cellpadding=0 cellspacing=0 width="85%" bgcolor=#f6f6ef><tr><td bgcolor=#ff6600><table border=0 cellpadding=0 cellspacing=0 width="100%" style="padding:2px"><tr><td style="width:18px;padding-right:4px"><a href="http://ycombinator.com"><img src="y18.gif" width=18 height=18 style="border:1px #ffffff solid;"></img></a></td><td style="line-height:12pt; height:10px;"><span class="pagetop"><b><a href="news">Hacker News</a></b><img src="s.gif" height=1 width=10><a href="newest">new</a> | <a href="threads?id=poof131">threads</a> | <a href="newcomments">comments</a> | <a href="ask">ask</a> | <a href="jobs">jobs</a> | <a href="submit">submit</a></span></td><td style="text-align:right;padding-right:4px;"><span class="pagetop"><a href="user?id=poof131">poof131</a>&nbsp;(1)&nbsp;|&nbsp;<a href="/r?fnid=0WqMDMmryZZg3JbdWvhsgf">logout</a></span></td></tr></table></td></tr><tr style="height:10px"></tr><tr><td><table border=0 cellpadding=0 cellspacing=0><tr><td align=right valign=top class="title">1.</td><td><center><a id=up_5531679 onclick="return vote(this)" href="vote?for=5531679&dir=up&by=poof131&auth=d8774c994d5eba19e42cbe718ab2ed74d2027506&whence=%6e%65%77%73"><img src="grayarrow.gif" border=0 vspace=3 hspace=2></a><span id=down_5531679></span></center></td><td class="title"><a href="http://net-security.org/secworld.php?id=14733">Hijacking airplanes with an Android phone</a><span class="comhead"> (net-security.org) </span></td></tr><tr><td colspan=2></td><td class="subtext"><span id=score_5531679>274 points</span> by <a href="user?id=mikeknoop">mikeknoop</a> 5 hours ago  | <a href="item?id=5531679">78 comments</a></td></tr><tr style="height:5px"></tr><tr><td align=right valign=top class="title">2.</td><td><center><a id=up_5533662 onclick="return vote(this)" href="vote?for=5533662&dir=up&by=poof131&auth=b145165316b6839450141e523d7f561d560dae1c&whence=%6e%65%77%73"><img src="grayarrow.gif" border=0 vspace=