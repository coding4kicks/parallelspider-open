function _init() {
    var cookie = readCookie("P7");
	if(document.referrer.match(/xfinity.comcast.net/)){
		document.cookie = ('P7=X;domain=.msnbc.msn.com;path=/;');
		isComcast();
	}
	else if(cookie !== null && cookie === "X"){
		if(document.referrer.match(/.msnbc.msn.com/)){
			isComcast();
		}
		else{
			showNetwork();
			deleteCookie();
		}
	}
	else{
		showNetwork();
	}
   }

   function isComcast() {
       $('body').addClass('comcast');
       showNetwork();
       $.getScript('http://xfinity.comcast.net/js-api/compressed/xpbar.js?version=3&pagename=msnbcheader&width=1100&id=xfinity');
   }

   function showNetwork() {
       $("#network .content").show();
   }

   function readCookie(ck) {
       var anyCookies = document.cookie;
       var pos = anyCookies.toUpperCase().indexOf(ck.toUpperCase() + "=");
       var value = "";
       if (pos != -1) {
           var start = pos + ck.length + 1;
           var end = anyCookies.indexOf(";", start);
           if (end == -1) {
               end = anyCookies.length;
           }
           value = anyCookies.substring(start, end);
           value = unescape(value);
       }
       return value;
   }

	function deleteCookie(){
		document.cookie = ('P7=X;expires=Thu, 01-Jan-1970 00:00:01 GMT');
	}

   _init();

</script></div></div><div id="bottomGradient" class="gradOff"></div></body></html>