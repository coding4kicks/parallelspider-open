    var AdBreakingNews = {};
    AdBreakingNews.Render = function () {
        try {
            if (AdBreakingNews.Enable ) {
    var marqueeColor = $('#marquee H1').css('backgroundColor');
    $('div#marquee:first').addClass('adBnm');
    $('div#marquee:first').prepend('<div id="AdBnmLeft"></div>').append('<div id="AdBnmRight"></div>');
    $('div#marqueeContainer').width('100%').css('background-color', marqueeColor);
    $('div#marquee').eq(1).empty().remove();
                if (AdBreakingNews.Left && AdBreakingNews.Left != "") {
                    dapMgr.enableACB("AdBnmLeft", false);
                    dapMgr.renderAd("AdBnmLeft", AdBreakingNews.Left, 120, 30);
                    AdBreakingNews.Rendered = true
                }
                if (AdBreakingNews.Right && AdBreakingNews.Right != "") {
                    dapMgr.enableACB("AdBnmRight", false);
                    dapMgr.renderAd("AdBnmRight", AdBreakingNews.Right, 120, 30);
                    AdBreakingNews.Rendered = true
                }
                $(window).resize(function () {
                    $(window).width() > 1158 ? $("div#marquee").addClass("adBnm") : $("div#marquee").removeClass("adBnm")
                })
            }
        } catch (a) {}
    };
    AdBreakingNews.InitAds = function () {
        AdBreakingNews.Left = "";
        AdBreakingNews.Right = "";
        AdBreakingNews.Width = 1024;
        AdBreakingNews.Enable = false;
        AdBreakingNews.Rendered = false;
        AdBreakingNews.Url = "/xml.aspx?id=39654701";
        $.get(AdBreakingNews.Url, function (a) {
            $(a).find("DisplayAd").each(function () {
                if ($(this).find("Enabled").text().toLowerCase() === "true") {
                    var d = $(this).find("AdLocation").text(),
                        b = $(this).find("Content").text(),
                        c = $(this).find("AdPlacementCode").text();
                    switch (d) {
                    case "AdBreakingNewsMarqueeLeft":
                        AdBreakingNews.Left = "&PG=" + b + "&AP=" + c;
                        AdBreakingNews.Enable = true;
                        break;
                    case "AdBreakingNewsMarqueeRight":
                        AdBreakingNews.Right = "&PG=" + b + "&AP=" + c;
                        AdBreakingNews.Enable = true
                    }
                }
            });
            if (AdBreakingNews.Enable && $(window).width() > AdBreakingNews.Width) AdBreakingNews.Render();
            else AdBreakingNews.Enable && $(window).resize(function () {
                AdBreakingNews.Enable && AdBreakingNews.Rendered === false && $(window).width() > AdBreakingNews.Width && AdBreakingNews.Render()
            })
        })
    }
